<form method="POST" action="{{!}}">
<table>
    
    <tr>
        <td>메뉴 이름:</td>
        <td><input name="dish_name" type="text" /></td>
    </tr>

    <tr>
        <td>최소 가격:</td>
        <td><input name="min_price" type="text" value="5.00" /></td>
    </tr>

    <tr>
        <td>최대 가격:</td>
        <td><input name="max_price" type="text" value="25.00" /></td>
    </tr>

    <tr>
        <td>맵기:</td>
        <td><select name="is_spicy"><option value="0">no</option><option value="1">yes</option><option value="2">either</option></select>        </td>
    </tr>

    <tr>
        <td colspan="2" align="center">
            <input name="search" value="검색" type="submit" /></td>
    </tr>
</table>
</form>
